<html>
  <head>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <h1><%=title %></h1>
    <ul className="messages">
      <% for (let i = 0; i < messages.length; i++) { %>
      <div><%= messages[i].user %></div>
      <br />
      <div><%= messages[i].text %></div>
      <br />
      <div><%= messages[i].added %></div>
      <br />
      <a href="/message/<%=i %>">Details</a>
      <br /><br />
      <% } %>
    </ul>
    <a href="/new">New Message</a>
  </body>
</html>
